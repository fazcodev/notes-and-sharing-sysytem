<!DOCTYPE html>
<html lang="en" class="lg:text-[95%] md:text-[90%] sm:text-[85%]">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css"
        integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
    
    <title>Seller Page</title>
    <base href="../public">
  <link rel="stylesheet" href="./assets-seller/index.5c5dfcbc.css">
</head>

<body class="bg-gray-100 ">
    <div class="wrapper w-11/12 m-auto">
        <header>
            <div class="mx-auto">
                <div class="flex justify-between item-center border-b-[0.15rem] border-slate-300 z-10">

                    <div class="flex items-center space-x-1 my-2">
                        <a class="item-center" href="/home"><img class="h-10" src="data:image/png;base64,"
                                alt="titles cart"></a>
                    </div>

                    <div class="flex md:hidden items-center space-x-1">
                        <a class="py-3 px-2" href="/">Home</a>
                        <a class="py-3 px-2" href="/contact">Contact</a>
                        <a class="py-3 px-2" href="/about">About</a>
                        <a class="py-3 px-2" href="/explore">Explore</a>
                    </div>

                    <div class="items-center flex space-x-1 my-3">

                        <a class="py-1 px-3 text-white bg-black rounded-full" href="/register">Register</a>
                        <a class="py-1 px-3 border border-black rounded-full hover:text-white hover:bg-black transition duration-500"
                            href="/login">Sign In</a>
                        <div class="hidden md:flex item-center">
                            <button class="mobile-menu-button py-1 pr-2 pl-4 duration-500 ease-in-out">
                                <div class="hamburger-line flex flex-col w-8 h-[1.38rem] justify-between items-center">
                                    <div
                                        class="line line-1 w-7 h-[0.15rem] bg-black duration-200 ease-in-out  origin-[0%]">
                                    </div>
                                    <div class="line line-2 w-7 h-[0.15rem] bg-black duration-100 ease-in-out"></div>
                                    <div
                                        class="line line-3 w-7 h-[0.15rem] bg-black duration-200 ease-in-out origin-[0]">
                                    </div>
                                </div>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="mobile-menu grid justify-items-center hidden ">
                <a class="block py-2 hover:bg-cyan-300 px-4" href="/">Home</a>
                <a class="block py-2 hover:bg-cyan-300 px-4" href="/contact">Contact</a>
                <a class="block py-2 hover:bg-cyan-300 px-4" href="/about">About</a>
                <a class="block py-2 hover:bg-cyan-300 px-4" href="/explore">Explore</a>
            </div>
        </header>
        <main>
            <section
                class="seller-detail bg-primary-blue mt-16 h-64 rounded-b-xl text-white flex justify-between px-3 py-2 md:mt-12">
                <div class="rating ">
                    <div class="likes"><i class="fa-solid fa-heart"></i> <span
                            class="like-number font-semibold">136</span> Likes</div>
                </div>
                <div class="info text-center mt-6 relative">
                    <h1 class="text-primary-blue-dark font-bold text-5xl"><%=row[0].Name%></h1>
                    <h4 class="mt-3">Authorize Seller</h4>
                    <p>We have wide range of computer science notes.</p>
                    <span class="absolute top-[58%] left-[24%] ">
                      
                        <img src="<%= row[0].Profile %>"
                            class="border-[0.6rem] border-gray-100 aspect-square rounded-full w-48" alt="profile"></span>
                </div>
                <ul>
                    <li><i class="fa-solid fa-signs-post"></i><span class="rate font-semibold pr-2"><%=rows.length%></span>Posts</li>
                    <li><i class="fa-solid fa-star"></i><span class="rate font-semibold pr-2"> 4.8</span>Seller Rating
                    </li>
                </ul>
            </section>
            <section class="posts mt-24 mb-24">
                <div class="inline-flex space-x-3 border-b-2 border-gray-500 pb-1 ml-4">
                    <img src="./assets-seller/Vector.ffd6f63e.svg" alt=""><span class="text-lg"> Posts</span>
                </div>
                <hr class="border-[0.1rem]">
                <div class="cards grid grid-cols-3 gap-8 mt-8 md:grid-cols-2 sm:grid-cols-1">
                    <%for(let i=0;i<rows.length;i++){%>
                    <div class="card flex flex-col rounded-xl shadow-lg shadow-gray-400 overflow-hidden">
                      <a href="/explore/<%=rows[i].id%>">  <img src="./post-images/<%=rows[i].images%>" class="object-cover w-full h-56" alt="post"></a>
                        <div class="detail bg-primary-blue-light py-4 px-5 text-primary-blue-dark">
                            <h1 class="font-bold text-2xl"><i class="fa-solid fa-indian-rupee-sign"></i><%= rows[i].price%></h1>
                            <p class="text-sm mt-3"><%=rows[i].description%></p>
                            <div class="flex justify-between mt-8 mb-2">
                                <h3 class="font-semibold"><%=rows[i].postdate%></h3>
                                <div>
                                    <i class="fa-solid fa-star"></i> 4.5/5</div>
                            </div>
                        </div>
                    </div>
                <%}%>

                  

                  
                </div>
            </section>
        </main>
    </div>
    <script type="text/javascript">
        const btn = document.querySelector(".mobile-menu-button");
        const menu = document.querySelector(".mobile-menu");
        const menuLines = document.querySelectorAll(".line");
        btn.addEventListener("click", () => {
            menu.classList.toggle("hidden");
            menuLines[0].classList.toggle("rotate-45");
            menuLines[1].classList.toggle("opacity-0");
            menuLines[2].classList.toggle("-rotate-45");

        });
    </script>
</body>

</html>